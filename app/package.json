{
  "name": "front-it",
  "private": true,
  "version": "0.0.0",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn lint && vite build && ./obfuscate.sh",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,json}\" ",
    "lint:types": "tsc --noEmit",
    "lint": "yarn lint:format && yarn lint:fix && yarn lint:types",
    "preview": "vite preview",
    "prebuild": "only-include-used-icons",
    "predev": "only-include-used-icons",
    "postinstall": "copy-dsfr-to-public && is-ci || cd .. && husky install app/.husky"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "0.78.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.4",
    "axios": "^1.2.3",
    "dayjs": "^1.11.7",
    "localstorage-slim": "^2.4.0",
    "react": "^18.2.0",
    "react-date-object": "^2.1.7",
    "react-dom": "^18.2.0",
    "react-multi-date-picker": "^4.4.0",
    "react-router-dom": "^6.6.2",
    "react-router-typesafe": "^1.1.0",
    "react-select": "^5.7.0",
    "recharts": "^2.7.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
